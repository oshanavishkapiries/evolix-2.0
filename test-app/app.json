{
  "expo": {
    "name": "Evolix",
    "slug": "evolix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.webp",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.webp",
        "backgroundColor": "#000000"
      },
      "package": "com.oshan_avishka.evolix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon.webp"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9587d2af-0159-43e3-96e9-d49e2a71b581"
      }
    },
    "owner": "oshan_avishka",
    "scheme": "evolix"
  }
}
